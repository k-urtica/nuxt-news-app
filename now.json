{
  "version": 2,
  "build": {
    "env": {
      "BASE_URL": "@base_url",
      "ANALYTICS_ID": "@analytics_id"
    }
  },
  "env": {
    "API_KEY": "@api_key"
  },
  "routes": [
    {
      "src": "/api/news.*",
      "dest": "/api/news.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ],
  "github": {
    "silent": true
  }
}